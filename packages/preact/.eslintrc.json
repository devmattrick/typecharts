{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "../../.eslintrc.json",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/no-unknown-property": ["error", { "ignore": ["class"] }]
  },
  "settings": {
    "react": {
      "version": "latest",
      "pragma": "h"
    }
  }
}
